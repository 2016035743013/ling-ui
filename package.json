{
  "name": "ling-ui",
  "version": "0.1.10",
  "private": false,
  "main": "lib/index.js",
  "module": "esm/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/2016035743013/ling-ui.git"
  },
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "3.4.3"
  },
  "files": [
    "esm",
    "lib"
  ],
  "scripts": {
    "start": "react-scripts start",
    "build": "npm run build:commonjs & npm run build:esm & npm run copy-css:esm & npm run copy-css:lib & npm publish",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "build:commonjs": "cross-env BABEL_OUTPUT=commonjs babel src/workspace --out-dir lib/ --ignore **/__tests__,**/__mocks__",
    "build:esm": "babel src/workspace --out-dir esm/ --ignore **/__tests__,**/__mocks__",
    "copy-css:esm": "cpx \"src/workspace/**/*.scss\" esm",
    "copy-css:lib": "cpx \"src/workspace/**/*.scss\" lib"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.4",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-react": "^7.10.4",
    "cpx": "^1.5.0",
    "cross-env": "^7.0.2",
    "cross-spawn": "^7.0.3",
    "node-sass": "^4.14.1",
    "prop-types": "^15.7.2",
    "react-scripts": "^3.4.3"
  }
}